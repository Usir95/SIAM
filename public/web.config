                        <add input="{HTTP_UPGRADE}" pattern="^WebSocket$" />
                    </conditions>
                    <action type="Rewrite" url="http://127.0.0.1:8086/{R:1}" />
                </rule> -->
            </rules>
        </rewrite>
        <handlers>
            <add name="php8228" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\php\php-8.2.28-Win32-vs16-x64\php-cgi.exe" resourceType="File" />
        </handlers>
       
    </system.webServer>
</configuration>